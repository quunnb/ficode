partial alphanumeric_keys
xkb_symbols "ficode" {

    name[Group1]="Finnish (Code)";

    key <TLDE> { [ grave,        asciitilde,  dead_tilde,      NoSymbol       ] };
    key <AE01> { [ 1,            exclam,      exclamdown,      NoSymbol       ] };
    key <AE02> { [ 2,            question,    at,              questiondown   ] };
    key <AE03> { [ 3,            numbersign,  sterling,        NoSymbol       ] };
    key <AE04> { [ 4,            dollar,      currency,        NoSymbol       ] };
    key <AE05> { [ 5,            percent,     permille,        NoSymbol       ] };
    key <AE06> { [ 6,            asciicircum, dead_circumflex, NoSymbol       ] };
    key <AE07> { [ 7,            ampersand,   NoSymbol,        NoSymbol       ] };
    key <AE08> { [ 8,            asterisk,    infinity,        NoSymbol       ] };
    key <AE09> { [ 9,            parenleft,   NoSymbol,        NoSymbol       ] };
    key <AE10> { [ 0,            parenright,  degree,          NoSymbol       ] };
    key <AE11> { [ minus,        underscore,  dead_macron,     NoSymbol       ] };
    key <AE12> { [ equal,        plus,        notequal,        plusminus      ] };

    key <AD01> { [ q,            Q,           NoSymbol,        NoSymbol       ] };
    key <AD02> { [ w,            W,           NoSymbol,        NoSymbol       ] };
    key <AD03> { [ e,            E,           EuroSign,        NoSymbol       ] };
    key <AD04> { [ r,            R,           Greek_ro,        Greek_RO       ] };
    key <AD05> { [ t,            T,           Greek_tau,       trademark      ] };
    key <AD06> { [ y,            Y,           yen,             NoSymbol       ] };
    key <AD07> { [ u,            U,           udiaeresis,      Udiaeresis     ] };
    key <AD08> { [ i,            I,           NoSymbol,        NoSymbol       ] };
    key <AD09> { [ o,            O,           aring,           Aring          ] };
    key <AD10> { [ p,            P,           Greek_pi,        Greek_PI       ] };
    key <AD11> { [ bracketleft,  braceleft,   NoSymbol,        NoSymbol       ] };
    key <AD12> { [ bracketright, braceright,  dead_acute,      NoSymbol       ] };

    key <AC01> { [ a,            A,           Greek_alpha,     Greek_ALPHA    ] };
    key <AC02> { [ s,            S,           U00DF,           U1E9E          ] };
    key <AC03> { [ d,            D,           Greek_delta,     Greek_DELTA    ] };
    key <AC04> { [ f,            F,           NoSymbol,        NoSymbol       ] };
